+static
+BOOL BfdDemangleSymName(LPCTSTR lpName, LPTSTR lpDemangledName, DWORD nSize)
+{
+	char *res;
+
+	assert(lpName != NULL);
+
+	if((res = cplus_demangle(lpName, DMGL_ANSI /*| DMGL_PARAMS*/)) == NULL)
+	{
+		lstrcpyn(lpDemangledName, lpName, nSize);
+		return FALSE;
+	}
+	else
+	{
+		lstrcpyn(lpDemangledName, res, nSize);
+		free (res);
+		return TRUE;
+	}
+}
